char* kangaroo(int x1, int v1, int x2, int v2) {

        int k1,k2=0;
        char *ans = "NO";

        do {
                x1 += v1;
                k1++;
                x2 += v2;
                k2++;

                if(k1 == k2 && x1 == x2) {
                        ans = "YES";                        
                }

        }while(x1<10000000 || x2 < 100000000);

        return ans;

}
