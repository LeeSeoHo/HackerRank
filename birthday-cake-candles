int birthdayCakeCandles(int ar_count, int* ar) {

        int cnt=0;

        int max = ar[1];
        for(int i=0;i<ar_count;i++) {
                if(max < ar[i]) {
                        max = ar[i];
                }
        }

        for(int j=0;j<ar_count;j++) {
                if(ar[j] == max) {
                        cnt++;
                }
        }

        return cnt;
}
