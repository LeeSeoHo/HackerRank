int countingValleys(int n, char* s) {

        int height = 0;

        for(int i=0;i<n;i++) {
                if(s[i] == 'U') {
                        height +=1;
                }
                else if(s[i] == 'D') {
                        height -= 1;
                }
                if (height == 0 && s[i]== 'U') {
                  ++height;
                }
        }      

        return height;
}
